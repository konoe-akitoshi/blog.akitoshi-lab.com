#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/akitoshi/Documents/GitHub/blog.akitoshi-lab.com/app/node_modules/.pnpm/drizzle-kit@0.31.1/node_modules/drizzle-kit/node_modules:/Users/akitoshi/Documents/GitHub/blog.akitoshi-lab.com/app/node_modules/.pnpm/drizzle-kit@0.31.1/node_modules:/Users/akitoshi/Documents/GitHub/blog.akitoshi-lab.com/app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/akitoshi/Documents/GitHub/blog.akitoshi-lab.com/app/node_modules/.pnpm/drizzle-kit@0.31.1/node_modules/drizzle-kit/node_modules:/Users/akitoshi/Documents/GitHub/blog.akitoshi-lab.com/app/node_modules/.pnpm/drizzle-kit@0.31.1/node_modules:/Users/akitoshi/Documents/GitHub/blog.akitoshi-lab.com/app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.cjs" "$@"
else
  exec node  "$basedir/../../bin.cjs" "$@"
fi
